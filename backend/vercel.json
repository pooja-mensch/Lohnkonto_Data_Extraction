{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ],
  "env": {
    "TEMPLATE_PATH": "template.xlsx",
    "ALLOWED_ORIGINS": "*"
  },
  "functions": {
    "api.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}
